{
    "name": "automatic-typer",
    "displayName": "Automatic Typer",
    "description": "Extens√£o que digita automaticamente um texto no editor, caractere por caractere.",
    "version": "0.0.1-test-delay",
    "publisher": "ejmeri",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.101.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.autoType"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.automaticTypeStart",
                "title": "Automatic Type: Iniciar"
            },
            {
                "command": "extension.automaticTypeStop",
                "title": "Automatic Type: Parar"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "typescript": "^4.7.4",
        "vscode": "^1.1.37",
        "vscode-test": "^1.6.1"
    }
}